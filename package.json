{
  "name": "terabyte-services",
  "version": "1.0.0",
  "description": "Terabyte Services - Web & App Development Company",
  "main": "index.js",
  "scripts": {
    "start": "live-server --port=3000 --host=localhost",
    "build": "npm run build:css && npm run build:js",
    "build:css": "postcss css/style.css -o dist/style.min.css --use autoprefixer cssnano",
    "build:js": "uglifyjs js/main.js -o dist/main.min.js --compress --mangle",
    "lint": "eslint js/**/*.js",
    "test": "echo \"Error: no test specified\" && exit 1"
